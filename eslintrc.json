{
    "extends": [
        "salt",
        "salt/react"
    ],
    "rules": {
        "mui/rule-name": 2,
        "react/jsx-closing-bracket-location": [
            "error",
            {
              "selfClosing": "after-props",
              "nonEmpty": "after-props"
            }
          ],
          "react/jsx-filename-extension": [
            "error",
            {
              "extensions": [
                ".js"
              ]
            }
          ],
          "react/prop-types": "off"
    },
    "plugins": ["mui"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }
  }